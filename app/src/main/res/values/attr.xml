        <attr name="minWidth" format="float" />
        <attr name="maxWidth" format="float" />
        <attr name="penColor" format="color" />
        <attr name="velocityFilterWeight" format="float" />
    </declare-styleable>

    <declare-styleable name="LabelView">
        <attr name="label_title" format="string" />
